alias rauto="nohup ruby automatic.rb &"
alias l.='ls -d .* --color=auto'
alias ll='ls -l --color=auto'
alias ls='ls --color=auto'
alias rvm-restart='rvm_reload_flag=1 source '\''/home/xyko/.rvm/scripts/rvm'\'''
alias start-nginx='sudo /usr/local/nginx/sbin/nginx'
alias start-t='transmission-daemon -er; sleep 2; transmission-remote -u 20  -gsr 0.01 -P'
alias start-unicorn='cd ~/xyko; unicorn_rails -c config/unicorn.rb -D'
alias stop-nginx='sudo /usr/local/nginx/sbin/nginx -s stop'
alias stop-t='killall  -9  transmission-daemon'
alias ta='transmission-remote -a'
alias tl='transmission-remote -l'
alias tlog1='tail -500f /home/xyko/octopus/a.log'
alias tlog2='tail -500f /home/xyko/octopus/nohup.out'
alias tt='transmission-remote'
alias vi='vim'
alias which='alias | /usr/bin/which --tty-only --read-alias --show-dot --show-tilde'
